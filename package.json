    "lint-staged": "^10.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-on-rails": "11.3.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "turbolinks": "^5.2.0"
  },
  "version": "0.1.0",
  "devDependencies": {
    "babel-eslint": "^10.0.3",
